	"type":"contao-module",
	"homepage":"https://rocksolidthemes.com/de/contao/plugins/responsive-slider",
	"license":"commercial",
	"authors":[
		{
			"name":"RockSolid Themes",
			"homepage":"https://rocksolidthemes.com/de/contao-themes",
			"role":"Developer"
		}
	],
	"support":{
		"forum":"http://help.rocksolidthemes.com/discussions/contao",
		"issues":"https://github.com/madeyourday/contao-rocksolid-slider/issues",
